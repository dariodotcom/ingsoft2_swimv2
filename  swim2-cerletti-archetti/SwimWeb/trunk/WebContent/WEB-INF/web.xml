<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <display-name>SwimWeb</display-name>
 <servlet>
  <display-name>Authentication</display-name>
  <servlet-name>Authentication</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.AuthenticationServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>PersonalPage</display-name>
  <servlet-name>PersonalPage</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.PersonalPageServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>CustomerFeedbacks</display-name>
  <servlet-name>CustomerFeedbacks</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.CustomerFeedbackServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>CustomerWorkRequests</display-name>
  <servlet-name>CustomerWorkRequests</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.CustomerWorkrequestServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>CustomerFriendship</display-name>
  <servlet-name>CustomerFriendship</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.CustomerFriendshipServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>GenericProfile</display-name>
  <servlet-name>GenericProfile</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.GenericProfileServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>Search</display-name>
  <servlet-name>Search</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.SearchServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>Administration</display-name>
  <servlet-name>Administration</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.AdministrationServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>MailValidation</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.MailValidationServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>PasswordReset</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.PasswordResetServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>AbilityRequest</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.AbilityRequestServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>CustomerImageServlet</display-name>
  <servlet-name>CustomerImageServlet</servlet-name>
  <servlet-class>it.polimi.swim.web.servlets.CustomerImageServlet</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>PersonalPage</servlet-name>
  <url-pattern>/home/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CustomerFeedbacks</servlet-name>
  <url-pattern>/feedbacks/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CustomerWorkRequests</servlet-name>
  <url-pattern>/works/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CustomerFriendship</servlet-name>
  <url-pattern>/friends/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>GenericProfile</servlet-name>
  <url-pattern>/user/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Administration</servlet-name>
  <url-pattern>/admin/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Search</servlet-name>
  <url-pattern>/search/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>MailValidation</servlet-name>
  <url-pattern>/validatemail/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CustomerImageServlet</servlet-name>
  <url-pattern>/userphoto/full.png</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CustomerImageServlet</servlet-name>
  <url-pattern>/userphoto/thumb.png</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Authentication</servlet-name>
  <url-pattern>/landing</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Authentication</servlet-name>
  <url-pattern>/login</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Authentication</servlet-name>
  <url-pattern>/logout</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Authentication</servlet-name>
  <url-pattern>/retry</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Authentication</servlet-name>
  <url-pattern>/register</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>Authentication</servlet-name>
  <url-pattern>/about</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>PasswordReset</servlet-name>
  <url-pattern>/resetpassword/*</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AbilityRequest</servlet-name>
  <url-pattern>/abilityrequest/*</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>landing</welcome-file>
 </welcome-file-list>
</web-app>
